{
  "name": "traffic simulator",
  "version": "0.0.1",
  "description": "A traffic simulator that allows you to plan traffic and which simulates its progress in time.",
  "scripts": {
    "clean": "rimraf ./dist",
    "dev": "npm start",
    "start": "npx rspack serve",
    "build": "npx rspack build",
    "build:domain": "npm run clean && npm run build"
  },
  "keywords": [
    "mithril",
    "materialize-css",
    "typescript"
  ],
  "license": "MIT",
  "app": "src/app.ts",
  "target": "web",
  "dependencies": {
    "maplibre-gl": "^4.1.0",
    "material-icons": "^1.13.12",
    "materialize-css": "^1.0.0",
    "meiosis-setup": "^6.2.3",
    "mithril": "^2.2.2",
    "mithril-materialized": "^1.1.4",
    "mithril-ui-form": "^1.10.4",
    "translate.js": "^1.3.2"
  },
  "devDependencies": {
    "@rspack/cli": "^0.5.5",
    "@rspack/core": "^0.5.5",
    "@types/materialize-css": "^1.0.14",
    "@types/mithril": "^2.2.6",
    "@types/geojson": "7946.0.14",
    "@types/mapbox-gl": "3.1.0",
    "dotenv": "16.4.5",
    "rimraf": "^5.0.5",
    "sass": "^1.71.1",
    "sass-loader": "14.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
